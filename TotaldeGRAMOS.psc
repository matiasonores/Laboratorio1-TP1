Proceso TotaldeGRAMOS
	Definir quimicoA, quimicoB, totalGramosA, totalGramosB, mililitrosPedidos como real;
	Escribir "Ingresar Cantidad Mililitros precisados";
	Leer mililitrosPedidos;
	quimicoA = 2.55/1000;
	quimicoB = 5.78/1000;
	totalGramosA = quimicoA*mililitrosPedidos;
	totalGramosB = quimicoB*mililitrosPedidos;
	Escribir "Se necesita:", totalGramosA,"gr del quimico A y:",totalGramosB,"gr del quimico B" ;
FinProceso
